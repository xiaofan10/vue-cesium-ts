    "postcss-pxtorem": "^6.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^1.8.6"
