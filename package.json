    "echarts": "^5.4.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "vite-plugin-cesium": "^1.2.22",
    "vue": "^3.3.4",
    "vue-color-kit": "^1.0.6",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
